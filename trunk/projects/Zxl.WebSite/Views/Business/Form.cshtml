@model Int32

<div id="div-@Model" style="overflow-y:scroll; width:100%; height:95%; background: #808080;">
</div>

<script type="text/javascript">
    $(function () {
        $.ajax({
            url: '@Url.Action("BuildForm", "Business")',
            data: { ProjectFormId: @Model },
            type: 'POST',
            success: function (content) {
                $("#div-@Model").append($.parseHTML(content));
            }
        });
    });
</script>