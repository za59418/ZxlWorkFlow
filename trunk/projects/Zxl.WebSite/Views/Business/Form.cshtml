@model Int32
<div style="overflow-y:hidden; width:100%; height:100%; background: #808080;">
    <div id="div-@Model" style="overflow-y:scroll; width:100%; height:96%; ">
    </div>
</div>
<script type="text/javascript">
    $(function () {
        $.ajax({
            url: '@Url.Action("BuildForm", "Business")',
            data: { ProjectFormId: @Model },
            type: 'POST',
            success: function (content) {
                $("#div-@Model").append($.parseHTML(content));
            }
        });
    });
</script>